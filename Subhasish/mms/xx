select `mms`.`GRN`.`srl` AS `srl`,`mms`.`GRN`.`GRN` AS `GRN`,`mms`.`GRN`.`GRDate` AS `GRDate`,`mms`.`GRN`.`i_code` AS `i_code`,`mms`.`GRN`.`s_code` AS `s_code`,`mms`.`GRN`.`Driver` AS `Driver`,`mms`.`GRN`.`TicketNo` AS `TicketNo`,`mms`.`GRN`.`Qty` AS `Qty`,`mms`.`GRN`.`UOM` AS `UOM`,`mms`.`GRN`.`BagWt` AS `BagWt`,`mms`.`GRN`.`Rate` AS `Rate`,`mms`.`item`.`i_name` AS `i_name`,`mms`.`Suppliers`.`s_name` AS `s_name` from ((`mms`.`GRN` left join `mms`.`item` on(`mms`.`GRN`.`i_code` = `mms`.`item`.`i_code`)) left join `mms`.`Suppliers` on(`mms`.`GRN`.`s_code` = `mms`.`Suppliers`.`s_code`))
